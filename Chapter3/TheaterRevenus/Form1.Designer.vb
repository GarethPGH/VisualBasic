    Private Sub calcButton_Click(sender As Object, e As EventArgs) Handles calcButton.Click
        Dim decAdultPrice As Decimal
        Dim decAdultSold As Decimal
        Dim decChildPrice As Decimal
        Dim decChildSold As Decimal
        'Calculated Values
        Dim decTotal As Decimal
        Dim decAdult As Decimal
        Dim decChild As Decimal

        Const decTHEATER_PERCENTAGE = 0.2D

        Try
            decAdultPrice = CDec(txtAdultPrice.Text)
            decAdultSold = CDec(txtAdultSold.Text)

            decChildPrice = CDec(txtChildPrice.Text)
            decChildSold = CDec(txtChildSold.Text)

            'Calculate Gross Values
            decAdult = decAdultSold * decAdultPrice
            txtGrossAdult.Text = decAdult.ToString("c")

            decChild = decChildSold * decChildPrice
            txtGrossChild.Text = decChild.ToString("c")

            decTotal = decChild + decAdult
            txtGrossTotal.Text = decTotal.ToString("c")

            'Calculate Net Values
            decAdult = decAdult * decTHEATER_PERCENTAGE
            txtNetAdult.Text = decAdult.ToString("c")

            decChild = decChild * decTHEATER_PERCENTAGE
            txtNetChild.Text = decChild.ToString("c")

            decTotal = decChild + decAdult
            txtNetTotal.Text = decTotal.ToString("c")

        Catch ex As Exception
            MessageBox.Show("Please enter Numerical values and Try Again")
        End Try
    End Sub

